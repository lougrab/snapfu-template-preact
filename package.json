{
	"name": "{{snapfu.name}}",
	"version": "0.0.1",
	"main": "index.js",
	"scripts": {
		"analyze": "webpack --config webpack.prod.js --analyze",
		"analyze:universal": "webpack --config webpack.prod.universal.js --analyze",
		"build": "rm -rf ./dist && webpack --config webpack.prod.js && webpack --config webpack.prod.universal.js",
		"cypress": "cypress open --project tests",
		"clean": "rm -rf ./node_modules && rm ./package-lock.json",
		"dev": "kill $(lsof -ti:3333,8888); webpack serve --config webpack.dev.js",
		"format": "prettier --write \"**/*.{js,ts,json}\"",
		"lint": "eslint --fix \"./src/**/*.{js,ts}\"",
		"test": "kill $(lsof -ti:3333,8888); webpack serve --config webpack.prod.js & (cypress run --project tests && kill $(lsof -ti:3333,8888))",
		"lighthouse": "lhci autorun --throttling.cpuSlowdownMultiplier=2.2",
		"prepare": "husky install"
	},
	"author": "{{snapfu.author}}",
	"searchspring": {
		"siteId": "{{snapfu.siteId}}",
		"framework": "preact",
		"tags": []
	},
	"license": "MIT",
	"prettier": "@searchspring/prettier",
	"dependencies": {
		"@searchspring/snap-preact": "^0.12.0",
		"@searchspring/snap-preact-components": "^0.12.0",
		"mobx": "^6.3.8",
		"mobx-react": "^7.2.1",
		"preact": "^10.6.2"
	},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/plugin-proposal-class-properties": "^7.16.0",
		"@babel/plugin-proposal-decorators": "^7.16.4",
		"@babel/plugin-transform-react-jsx": "^7.16.0",
		"@babel/plugin-transform-runtime": "^7.16.4",
		"@babel/preset-env": "^7.16.4",
		"@babel/preset-react": "^7.16.0",
		"@babel/runtime": "^7.16.3",
		"@lhci/cli": "^0.8.2",
		"@prefresh/babel-plugin": "^0.4.1",
		"@prefresh/webpack": "^3.3.2",
		"@searchspring/browserslist-config-snap": "1.0.4",
		"@searchspring/prettier": "^1.0.2",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.3",
		"core-js": "^3.19.3",
		"css-loader": "^6.5.1",
		"cypress": "8.5.0",
		"eslint": "^8.4.1",
		"eslint-plugin-react": "^7.27.1",
		"file-loader": "^6.2.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.1.2",
		"prettier": "^2.5.1",
		"sass": "^1.44.0",
		"sass-loader": "^12.4.0",
		"style-loader": "^3.3.1",
		"webpack": "^5.65.0",
		"webpack-bundle-analyzer": "^4.5.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.6.0",
		"webpack-merge": "^5.8.0"
	},
	"lint-staged": {
		"./package.json": [
			"prettier --write"
		],
		"./src/**/*.{js,jsx,ts,tsx}": [
			"prettier --write",
			"eslint --fix"
		]
	},
	"browserslist": {
		"universal": "extends @searchspring/browserslist-config-snap/universal",
		"modern": "extends @searchspring/browserslist-config-snap/modern"
	}
}
